.list-content {
	border: 1px solid #ccc;
}
.list-content-title {
	line-height: 24px;
	padding: 0 10px;
	background: #f44;
	color: #fff;
	font-size: 12px;
}

.list-content-list {
	font-size: 12px;
	padding: 10px 15px;
}

.list-content-item {
	font-size: 12px;
	line-height: 16px;
	padding: 5px 0;
	color: #333;
}
.list-content-time {
	padding-left: 5px;
}
.listinfo_container {
	width: 1024px;
	position: relative;
}
.Barchart_container {
	position: absolute;
	top: 50px;
	right: 10px;
}